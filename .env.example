# Youtu-agent Environment Variables Configuration
# Copy this file to .env and fill in your actual values

# =============================================================================
# MAIN LLM CONFIGURATION (Required)
# =============================================================================
# Primary language model configuration for the agent
UTU_LLM_TYPE=openai                    # LLM provider (openai, deepseek, etc.)
UTU_LLM_MODEL=gpt-4o-mini             # Model name/version
UTU_LLM_BASE_URL=https://api.openai.com/v1  # API base URL
UTU_LLM_API_KEY=your_openai_api_key_here    # Your API key

# =============================================================================
# JUDGE LLM CONFIGURATION (Optional - for evaluation)
# =============================================================================
# Separate model for evaluation/judging (can use same as main if desired)
JUDGE_LLM_TYPE=openai
JUDGE_LLM_MODEL=gpt-4o-mini
JUDGE_LLM_BASE_URL=https://api.openai.com/v1
JUDGE_LLM_API_KEY=your_judge_api_key_here

# =============================================================================
# SEARCH & WEB TOOLS
# =============================================================================
# Required for web search functionality
SERPER_API_KEY=your_serper_api_key_here     # Google search API key
JINA_API_KEY=your_jina_api_key_here         # Jina AI API key for embeddings

# =============================================================================
# GITHUB INTEGRATION
# =============================================================================
# Required for GitHub tool functionality
GITHUB_TOKEN=your_github_personal_access_token

# =============================================================================
# MICROSOFT/OUTLOOK CALENDAR INTEGRATION
# =============================================================================
# Required for Outlook calendar functionality
OUTLOOK_CLIENT_ID=your_outlook_client_id
OUTLOOK_CLIENT_SECRET=your_outlook_client_secret
OUTLOOK_TENANT_ID=your_outlook_tenant_id
OUTLOOK_USER_ID=user@domain.com              # Optional: specific user ID
# OUTLOOK_ACCESS_TOKEN=your_access_token     # Alternative to client credentials

# =============================================================================
# MICROSOFT TEAMS CALENDAR INTEGRATION
# =============================================================================
# Required for Teams calendar functionality
TEAMS_CLIENT_ID=your_teams_client_id
TEAMS_CLIENT_SECRET=your_teams_client_secret
TEAMS_TENANT_ID=your_teams_tenant_id
# TEAMS_ACCESS_TOKEN=your_access_token       # Alternative to client credentials

# =============================================================================
# GENERAL MICROSOFT CONFIGURATION (Alternative naming)
# =============================================================================
# Alternative environment variable names for Microsoft services
MS_CLIENT_ID=your_ms_client_id
MS_CLIENT_SECRET=your_ms_client_secret
MS_TENANT_ID=your_ms_tenant_id

# =============================================================================
# GOOGLE SERVICES
# =============================================================================
# Required for video/image processing
GOOGLE_API_KEY=your_google_api_key
GOOGLE_MODEL=gemini-1.5-flash              # Model for Google services

# =============================================================================
# SPECIALIZED TOOLS
# =============================================================================
# Image processing model (can use same as main LLM)
UTU_TOOL_IMAGE_MODEL=gpt-4o-mini

# Audio processing API
AUDIO_API_KEY=your_audio_api_key
AUDIO_BASE_URL=https://api.audio-service.com

# Document processing (optional)
# CHUNKR_API_KEY=your_chunkr_api_key

# Code snippet service (optional)
# CODESNIP_SERVER_URL=https://your-codesnip-server.com

# Remote shell access (optional)
# REMOTE_SHELL_URL=https://your-remote-shell.com
# REMOTE_SHELL_KEY=your_remote_shell_key

# LinkedIn integration (optional)
# LINKEDIN_API_KEY=your_linkedin_api_key

# =============================================================================
# DATABASE CONFIGURATION
# =============================================================================
# Database connection for tracing and caching
DB_URL=sqlite:///utu.db                    # SQLite for local development
# DB_URL=postgresql://user:password@localhost:5432/utu  # PostgreSQL example

# =============================================================================
# TRACING & MONITORING
# =============================================================================
# Phoenix tracing (optional - for monitoring agent performance)
# PHOENIX_ENDPOINT=http://localhost:6006/v1/traces
# PHOENIX_PROJECT_NAME=Youtu-agent

# OpenTelemetry logging level (optional)
# OTEL_LOG_LEVEL=INFO

# =============================================================================
# MEMORY & STORAGE
# =============================================================================
# Example memory file path (optional)
# UTU_EXAMPLE_MEMORY_FILE_PATH=./memory/example_memory.json

# =============================================================================
# FRONTEND (Next.js/React) - JAVASCRIPT ENVIRONMENT VARIABLES
# =============================================================================

# SUPABASE CONFIGURATION (Required for authentication and database)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_PUBLISHABLE_OR_ANON_KEY=your_supabase_anon_key

# CAREER COACH WEBSOCKET (connects frontend to Youtu-agent backend)
CAREER_COACH_WS_URL=ws://127.0.0.1:8848/ws

# VERCEL DEPLOYMENT (automatically set by Vercel)
# VERCEL_URL=your-app.vercel.app
# VERCEL_ENV=development|preview|production

# NODE ENVIRONMENT
NODE_ENV=development
